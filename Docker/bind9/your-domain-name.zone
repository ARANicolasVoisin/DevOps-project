; BIND data file for your.domain.name
$TTL    86400
@       IN      SOA     ns1.your.domain.name. root.your.domain.name.
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@               IN      NS      your.domain.name.

ns1       IN      A       192.168.1.200
